--     * Slot: Container_id Description: Autocreated FK slot
-- # Class: "Condition_occurrence" Description: ""
--     * Slot: condition_occurrence_id Description: The unique key given to a condition record for a person. Refer to the ETL for how duplicate conditions during the same visit were handled.
--     * Slot: person_id Description: It is assumed that every person with a different unique identifier is in fact a different person and should be treated independently.
--     * Slot: condition_source_value Description: This field houses the verbatim value from the source data representing the condition that occurred. For example, this could be an ICD10 or Read code.
--     * Slot: Container_id Description: Autocreated FK slot
-- # Class: "Container" Description: ""
--     * Slot: id Description: 

CREATE TABLE "Container" (
	id INTEGER NOT NULL, 
	PRIMARY KEY (id)
);
CREATE TABLE "Person" (
	person_id INTEGER NOT NULL, 
	year_of_birth INTEGER, 
	gender_concept_id INTEGER, 
	"Container_id" INTEGER, 
	PRIMARY KEY (person_id), 
	FOREIGN KEY("Container_id") REFERENCES "Container" (id)
);
CREATE TABLE "Condition_occurrence" (
	condition_occurrence_id INTEGER NOT NULL, 
	person_id INTEGER, 
	condition_source_value TEXT, 
	"Container_id" INTEGER, 
	PRIMARY KEY (condition_occurrence_id), 
	FOREIGN KEY("Container_id") REFERENCES "Container" (id)
);
